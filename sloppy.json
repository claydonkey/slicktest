{
    "project": "slick-project",
    "services": [
	{
	    "id": "slick",
	    "apps": [
		{
		    "env": {},
		    "domain": {
			"uri": "slicktest.sloppy.zone"
		    },
		    "mem": 512,
		    "image": "claydonkey/sloppy-slick",
		    "instances": 1,
		    "id": "slickapp",
		    "health_checks": [],
		    "volumes": [],
		    "port_mappings": [
			{
			    "container_port": 9000
			}
		    ],
		    "dependencies": [
			"../../backend/mysql"
		    ]
		}
	    ]
	},
	{
	    "id": "backend",
	    "apps": [
		{
		    "id": "mysql",
		    "instances": 1,
		    "mem": 512,
		    "port_mappings": [
			{
			    "container_port": 3306,
			    "protocol": "tcp"
			}
		    ],
		    "image": "claydonkey/mysql",
		    "_comment volumes": [
			{
			    "container_path": "/var/lib/mysql",
			    "size": "8GB"
			}
		    ],
		    "domain": {
			"uri": "claydonkeysql.sloppy.zone"
		    },
		    "env": {
			"MYSQL_ROOT_PASSWORD": "example",
			"MYSQL_USER": "",
			"MYSQL_PASSWORD": "example",
			"MYSQL_DATABASE": "slicktest"
		    }
		}
	    ]
	}
    ]
}

